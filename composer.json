{
    "name": "ngabor84/ecc",
    "description": "Command line tool for check environment variables consistency between several environments",
    "keywords": ["cli", "environment variable", "consistency", "checker"],
    "homepage": "https://github.com/ngabor84/ecc",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor Nemeth",
            "email": "negabor@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/console": "~3.1",
        "josegonzalez/dotenv": "^3.2",
        "league/climate": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": ">=3.2.3",
        "kherge/box": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Ecc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecc\\Test\\": "test/"
        }
    }
}
